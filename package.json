{
  "name": "fabella-engine-api-sdk",
  "version": "0.1.0",
  "private": false,
  "description": "TypeScript SDK for Fabella Engine API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts"
  },
  "files": ["dist/"],
  "dependencies": {
    "axios": "^1.6.2",
    "core-js": "^3.8.3"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.6",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "jsdom": "^22.1.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.0",
    "typescript": "~4.5.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "author": "Wanderword",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wanderword/fabella-engine-api-sdk.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
